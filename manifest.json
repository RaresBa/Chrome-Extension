{
    "manifest_version": 3,
    "name": "Web Page Translator and Reverser",
    "version": "1.0",
    "description": "Translate web pages or reverse the text on them.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": [
      "scripting",
      "activeTab"
    ]
  }
